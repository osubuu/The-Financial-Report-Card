    "eslint-plugin-react": "^7.14.2",
    "firebase": "^5.11.1",
    "lodash": "^4.17.11",
    "react-autocomplete": "^1.8.1",
    "react-chartjs-2": "^2.7.6",
    "react-loading-screen": "0.0.17",
